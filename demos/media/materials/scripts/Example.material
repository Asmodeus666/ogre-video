material Examples/CloudySky
{
	technique
	{
		pass
		{
			lighting off
			depth_write off

			texture_unit
			{
				texture clouds.jpg
				scroll_anim 0.15 0
			}
		}
	}
}

material SimpleVideo
{
	technique
	{
		pass
		{
			cull_software none
			cull_hardware none
			lighting off
			texture_unit
			{
				texture_source ogg_video
				{
					filename clip.ogg
					precache 50
					play_mode play
				}
			}
		}
	}
}
